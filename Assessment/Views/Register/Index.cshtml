@model IEnumerable<RegisterViewModel>



<div class="container p-3">
    <div class="row pt-4">
        @if (@TempData["success"] != null)
        {
            <h5 class="text-start">@TempData["success"]</h5>
        }
    </div>

    <div class="row pt-4">
        <div class="col-6">
            <h2 class="test-primary">Email List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Register" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> Enter Mail
            </a>
        </div>
    </div>
</div>
<br />
<br />
<table class="table talble-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.FirstName
                </td>
                <td>
                    @obj.LastName
                </td>
                <td>
                    @obj.Email
                </td>
            </tr>
        }
    </tbody>

</table>
